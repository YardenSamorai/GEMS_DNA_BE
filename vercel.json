{
    "version": 2,
    "builds": [
      {
        "src": "api/stones/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/soap-stones",
        "dest": "api/stones/server.js"
      },
      {
        "src": "/api/tags",
        "dest": "api/stones/server.js"
      },
      {
        "src": "/api/tags/(.*)",
        "dest": "api/stones/server.js"
      },
      {
        "src": "/api/stone-tags",
        "dest": "api/stones/server.js"
      },
      {
        "src": "/api/stones",
        "dest": "api/stones/server.js"
      },
      {
        "src": "/api/stones/(.*)",
        "dest": "api/stones/server.js"
      }
    ]
  }
  